{
  "name": "tm-barcode-qrcode-generator",
  "description": "Barcode and QR code generator, that works offline",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "license": "",
  "author": "Mikkel Albrechtsen",
  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/vite.svg",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "i18next": "^24.2.0",
    "react": "^18.3.0",
    "react-barcode": "^1.5.3",
    "react-dom": "^18.3.0",
    "react-i18next": "^15.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
